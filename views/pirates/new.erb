     <h1>Pirate's Weight</h1><input type="text" name="pirate[weight]">
     <h1>Pirate's Height</h1><input type="text" name="pirate[height]">
     <h1>Ship 1's Name</h1><input id="ship_name_1" type="text" name="pirate[ships][][name]">
     <h1>Ship 1's Type</h1><input id="ship_type_1" type="text" name="pirate[ships][][type]">
     <h1>Ship 1's Booty</h1><input id="ship_booty_1" type="text" name="pirate[ships][][booty]">
     <h1>Ship 2's Name</h1><input id="ship_name_2" type="text" name="pirate[ships][][name]">
     <h1>Ship 2's Type</h1><input id="ship_type_2" type="text" name="pirate[ships][][type]">
     <h1>Ship 2's Booty</h1><input id="ship_booty_2" type="text" name="pirate[ships][][booty]">
     <button value="Submit">
